<h1>Game#List</h1>
<table>
  <tr>
    <th>id</th>
    <th>date</th>
    <th>player 1</th>
    <th>player 2</th>
    <th>player 3</th>
    <th>player 4</th>
  </tr>
  <% @games.each do |game| %>
  <tr>
    <td><%= game.id %></td>
    <td><%= game.created_at.localtime("+08:00").strftime("%Y-%m-%d %T") %></td>
    <td><% player_info(game.players[0]) %></td>
    <td><% player_info(game.players[1]) %></td>
    <td><% player_info(game.players[2]) %></td>
    <td><% player_info(game.players[3]) %></td>
  </tr>
  <% end %>
</table>

